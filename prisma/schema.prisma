generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Quotation {
  id              Int      @id @default(autoincrement())
  quotationNumber String   @unique
  customerData    Json
  items           Json
  total           Float
  conditions      String?  @db.Text
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}
